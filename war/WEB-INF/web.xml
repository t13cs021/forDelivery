<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>Recommend</servlet-name>
		<servlet-class>com.appspot.projectF.servlet.RecommendServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Detail</servlet-name>
		<servlet-class>com.appspot.projectF.servlet.DetailServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Graph</servlet-name>
		<servlet-class>com.appspot.projectF.servlet.GraphServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Recommend</servlet-name>
		<url-pattern>/recommend</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Detail</servlet-name>
		<url-pattern>/detail</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Graph</servlet-name>
		<url-pattern>/graph</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>